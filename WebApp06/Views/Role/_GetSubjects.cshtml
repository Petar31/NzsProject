@model List<WebApp06.Models.Test.Subject>

<div class="row">
    <div class="col-sm-9">
        <table class="table">
            <tr>
                <th>Subjects</th>
                <th></th>
            </tr>

            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.Name</td>
                    <td>
                        <a asp-action="DeleteRole" asp-route-id="@item.Id">Delete subject</a>
                    </td>
                </tr>
            }
        </table>
    </div>

</div>


